<h2>Workout Program: <%= workout.title %></h2>
<img src="<%= workout.image_url%>" alt="">

<% console.log('userId:', userId); console.log('workout.userId:', workout.user_id); %>

<% if(userId && workout.user_id === userId) { %>
    <form action="/workouts/<%= workout.id %>" method="post">
        <input type="hidden" name="_method" value="delete">
        <button>Delete</button>
    </form>
    <a href="/workouts/<%= workout.id %>/edit">Edit Workout</a>
    <% } else if (!userId) { %>
<form action="/login" method="post">
<button>login</button>
</form>
<% } %>